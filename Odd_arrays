Public Sub Main()

Dim a As New Integer[5]
Dim b As New Integer[5]
Dim x, y As Integer

For x = 0 To 4
  Print "Give me a number"
  Input a[x]
Next

For x = 0 To 4
  If a[x] Mod 2 = 1 Then
    b[y] = a[x]
    y = y + 1
  Endif
Next

Print "odd arrays"

For x = 0 To 4
  If b[x] <> 0 Then
  Print b[x]
  End If
Next

End
